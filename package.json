{
	"name": "cinelog_web",
	"private": true,
	"version": "0.0.0",
	"type": "module",
	"scripts": {
		"prepare": "git config core.hooksPath .githooks",
		"dev": "vite",
		"build": "tsc -b tsconfig.app.json tsconfig.node.json && vite build",
		"lint": "biome check",
		"lint:fix": "biome check --write",
		"preview": "vite preview",
		"generate:feature": "plop --plopfile scripts/plop/plopfile.js feature",
		"generate:component": "plop --plopfile scripts/plop/plopfile.js component",
		"test": "vitest",
		"test:ui": "vitest --ui",
		"test:coverage": "vitest run --coverage",
		"test:unit": "VITEST_TEST_TYPE=unit vitest run",
		"test:unit:coverage": "VITEST_TEST_TYPE=unit vitest run --coverage",
		"test:integration": "VITEST_TEST_TYPE=integration vitest run",
		"test:integration:coverage": "VITEST_TEST_TYPE=integration vitest run --coverage"
	},
	"dependencies": {
		"@antoniobenincasa/ui": "^0.10.0",
		"@hookform/resolvers": "^5.2.2",
		"i18next": "^25.7.4",
		"i18next-browser-languagedetector": "^8.2.0",
		"ky": "^1.14.3",
		"lucide-react": "^0.562.0",
		"react": "^19.2.3",
		"react-dom": "^19.2.3",
		"react-hook-form": "^7.71.1",
		"react-i18next": "^16.5.3",
		"react-router-dom": "^7.12.0",
		"tailwindcss": "^4.1.17",
		"zod": "^4.1.13",
		"zustand": "^5.0.10"
	},
	"devDependencies": {
		"@biomejs/biome": "^2.3.11",
		"@playwright/test": "^1.57.0",
		"@tailwindcss/vite": "^4.1.17",
		"@testing-library/jest-dom": "^6.9.1",
		"@testing-library/react": "^16.3.1",
		"@testing-library/user-event": "^14.6.1",
		"@types/node": "^25.0.8",
		"@types/react": "^19.2.8",
		"@types/react-dom": "^19.2.3",
		"@vitejs/plugin-react": "^5.1.2",
		"@vitest/coverage-v8": "^4.0.16",
		"@vitest/ui": "^4.0.16",
		"fuzzy": "^0.1.3",
		"globals": "^17.0.0",
		"inquirer-autocomplete-prompt": "^3.0.1",
		"jsdom": "^27.4.0",
		"plop": "^4.0.4",
		"tw-animate-css": "^1.4.0",
		"typescript": "~5.9.3",
		"vite": "npm:rolldown-vite@7.3.1",
		"vitest": "^4.0.17"
	},
	"overrides": {
		"vite": "npm:rolldown-vite@7.3.1"
	}
}
